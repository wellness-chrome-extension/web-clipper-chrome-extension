{
    "manifest_version": 2,
    "name": "APP_NAME",
    "description": "DESCRIPTION",
    "version": "1.0.0",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["jquery-3.4.1.min.js", "content.js"]
      }
    ]
  }