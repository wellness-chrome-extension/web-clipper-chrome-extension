{
    "manifest_version": 2,
    "name": "Inspiration List",
    "description": "DESCRIPTION",
    "options_page": "profile.html",
    "version": "1.0.0",
    "background": {
      "scripts": ["sample.js", "popup.js"]
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": { "16": "icon.png",
      "48": "icon.png",
     "128": "icon.png" },
    "permissions": ["activeTab", "contextMenus", "storage"],

    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["jquery-3.4.1.min.js", "content.js"]
      }
    ]
  }