{
    "manifest_version": 2,
    "name": "Web Clipper",
    "description": "Right click and save your favorite parts of the internet in this handy, simple chrome extension.",
    "options_page": "profile.html",
    "version": "1.0.0",
    "background": {
      "scripts": ["sample.js", "popup.js"]
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": { "16": "icon.png",
      "48": "icon.png",
     "128": "icon.png" },
    "permissions": ["activeTab", "contextMenus", "storage"]
  }